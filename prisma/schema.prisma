// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Storage {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  user    String @unique
  type    String @unique
  content Json
}
